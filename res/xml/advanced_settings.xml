<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" android:title="">

    <!-- BuildProp Modder -->
    <PreferenceScreen
        android:key="buildprop_modder"
        android:fragment="com.android.settings.cna.PropModder"
        android:title="@string/propmodder_title" />

    <!-- Spare Parts Settings -->
    <PreferenceScreen
        android:key="spareparts_settings"
        android:title="@string/spareparts_settings_title">
        <intent
            android:action="android.intent.action.MAIN"
            android:targetPackage="com.android.spare_parts"
            android:targetClass="com.android.spare_parts.SpareParts" />
    </PreferenceScreen>

    <!-- Development Settings -->   
    <PreferenceScreen
        android:key="developments_settings"
        android:title="@string/developments_settings_title">
        <intent
            android:action="android.intent.action.MAIN"
            android:targetPackage="com.android.development"
            android:targetClass="com.android.development.Development" />
    </PreferenceScreen>

    <!-- Testing Settings -->   
    <PreferenceScreen
        android:key="testing_settings"
        android:fragment="com.android.settings.TestingSettings"
        android:title="@string/testing_settings_title" />

</PreferenceScreen>
